TRIGGER "SET_VERSIONS"
BEFORE UPDATE ON "DB_DATA" 
REFERENCING NEW ROW MYNEWROW, OLD ROW MYOLDROW
FOR EACH ROW
BEGIN
    DECLARE sequenceVersion INTEGER;
    SELECT DATA_VERSION_SEQUENCE.NEXTVAL INTO sequenceVersion FROM DUMMY;
    MYNEWROW.SEQUENCEVERSION = :sequenceVersion;
    MYNEWROW.VERSION = :MYOLDROW.VERSION + 1;
END
